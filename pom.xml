            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-failsafe-plugin</artifactId>
                <configuration>
                    <includes>
                        <include>**/*IntegrationTest.java</include>
                        <include>**/*RESTfulServiceTest.java</include>
                        <include>**/*RESTfulTest.java</include>
                        <include>**/*RestfulTest.java</include>
                    </includes>
                </configuration>
                <executions>
                    <execution>
