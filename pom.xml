                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-pmd-plugin</artifactId>
                <configuration>
                    <rulesets>
                        <ruleset>http://eselivm2v214l.lmera.ericsson.se:8081/nexus/service/local/repositories/oex-interim/content/com/ericsson/oss/build/support/QRank3_8_wrancm_ruleset/1.0.0/QRank3_8_wrancm_ruleset-1.0.0.xml</ruleset>
                    </rulesets>
                    <targetJdk>1.6</targetJdk>
                    <excludes>
                        <exclude>**/src/test/*.java</exclude>
                    </excludes>
                    <includeTests>false</includeTests>
                    <sourceEncoding>utf-8</sourceEncoding>
                </configuration>
                <executions>
                    <execution>
                        <phase>test</phase>
                        <goals>
                            <goal>pmd</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <!-- Duplicated below. Causes nasty warning. plugin> <groupId>org.codehaus.mojo</groupId> 
                <artifactId>cobertura-maven-plugin</artifactId> <configuration> <formats>
                <format>xml</format> </formats> </configuration> <executions> <execution>
                <phase>none</phase> </execution> </executions> </plugin -->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>cobertura-maven-plugin</artifactId>
                <configuration>
                    <formats>
                        <format>xml</format>
                    </formats>
                    <instrumentation>
                        <excludes>
                            <exclude>com/ericsson/eniq/events/server/test/**/*.class</exclude>
                        </excludes>
                    </instrumentation>
                </configuration>
                <executions>
                    <execution>
                        <phase>none</phase>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-reactor-plugin</artifactId>
                <version>1.0</version>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>versions-maven-plugin</artifactId>
                <configuration>
                    <includes>
                        <include>com.ericsson.eniq:*</include>
                        <include>com.distocraft.dc5000:*</include>
                    </includes>
                    <excludes>
                        <exclude>com.ericsson.eniq.events:eniq-events-parent:pom:12.0-SNAPSHOT</exclude>
                        <exclude>org.jmock:*</exclude>
                        <exclude>org.eclipse.persistence:*</exclude>
